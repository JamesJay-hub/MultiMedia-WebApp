{
  "builds": [
    {
      "src": "multimedia_webapp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "multimedia_webapp/wsgi.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python@3.12",
      "config": {
        "excludeFiles": [".next/cache/**/*", "node_modules/**/*"]
      }
    }
  }
}
